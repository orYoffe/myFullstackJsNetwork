{
  "name": "my-fullstack-js-network",
  "version": "0.0.1",
  "private": true,
  "description": "\u0016This is an ```Express``` app using react, redux, react-router, Es6, and server rendering.",
  "main": "./app/dist/server.js",
  "scripts": {
    "mongo": "cd /c/MongoDB/Server/3.2/bin && ./mongod",
    "build": "babel ./app/src -d ./app/dist && webpack -p --progress",
    "serve": "node ./app/dist/server.js",
    "test": "mocha --compilers js:babel-register",
    "start": "npm run build && npm run serve",
    "dev-server": "nodemon ./app/src/server.js --exec babel-node --presets es2015,stage-2",
    "dev-webpack": "webpack --watch --progress -d --config webpack.config.dev.js",
    "dev1": "npm run dev-server",
    "dev2": "npm run dev-webpack"
  },
